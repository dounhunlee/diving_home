<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="stylesheet" href="./style/gnb_all.css">
  <link rel="stylesheet" href="./style/style.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
  <script src="./js/gnb_all.js"></script> 
  <script src="./js/script.js"></script> 
  <script src="./js/script_1.js"></script> 
</head>
<body>
 

    

<%- include('nav_component') %>








  <div id = "login_info">
    <div id = "login_area">
      <div id = "login_title">로그인</div>
      <form id="login_form" action="/login" method="POST">
            <!-- <h1>사전</h1> -->
          <div id = "input_area">
            
            <div class = "input_style input_id">
            
              <input type="text" id="id" name="id" required autocomplete="off" placeholder="아이디">
            </div>
      
            <div class = "input_style input_pw">
              
              <input type="password" id="pwd" name="pwd" required autocomplete="off" placeholder="비밀번호">
            </div>
      
          </div>
           
            <div class = "btn_area">
                 
            <button type="submit" class="btn_login">로그인</button>
            
      
            </div>
      
      </form>
    </div>
</div>
</body>
</html>